<div id="tea-carousel" class="carousel slide" data-ride="carousel">
    <ul class="carousel-indicators">
        <li data-target="#tea-carousel" data-slide-to="0" class="active"></li>
        <li data-target="#tea-carousel" data-slide-to="1"></li>
        <li data-target="#tea-carousel" data-slide-to="2"></li>
    </ul>

    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="slide-image" src="~/Images/Slideshow/tea1.jpg" alt="">
        </div>
        <div class="carousel-item">
            <img class="slide-image" src="~/Images/Slideshow/tea2.jpg" alt="">
        </div>
        <div class="carousel-item">
            <img class="slide-image" src="~/Images/Slideshow/tea3.jpg" alt="">
        </div>
        <div class="carousel-item">
            <img class="slide-image" src="~/Images/Slideshow/tea4.jpg" alt="">
        </div>
    </div>
    <a class="carousel-control-prev" href="#tea-carousel" data-slide="prev">
        <span class="carousel-control-prev-icon"></span>
    </a>
    <a class="carousel-control-next" href="#tea-carousel" data-slide="next">
        <span class="carousel-control-next-icon"></span>
    </a>
</div>

